{
  "name": "nest-morgan",
  "version": "2.0.0",
  "description": "Morgan Module for Nest Framework",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && cp *.md dist && cp LICENSE dist && cp package.json dist",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mentos1386/nest-morgan.git"
  },
  "keywords": [
    "nestjs",
    "nest",
    "morgan",
    "logging",
    "module"
  ],
  "author": "Tine Jozelj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mentos1386/nest-morgan/issues"
  },
  "homepage": "https://github.com/mentos1386/nest-morgan#readme",
  "dependencies": {
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@nestjs/common": "^7.6.13",
    "@nestjs/core": "^7.6.13",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/testing": "^7.6.13",
    "@types/jest": "^26.0.20",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.33",
    "coveralls": "^3.1.0",
    "jest": "^26.6.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.6",
    "rxjs-compat": "^6.6.6",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
